{
  "name": "jf-crm",
  "version": "1.0.0",
  "description": "JF CRM",
  "scripts": {
    "build": "babel client/app -d client/js && gulp && tsc && npm run lint",
    "dependencies": "npm install && bower install",
    "clean": "rimraf ./dist",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "tsc": "tsc",
    "start": "node dist/server/app.js",
    "launch": "npm run build && node --nolazy --inspect-brk=9229 dist/server/app.js"
  },
